    "@types/node": "^11.13.0",
    "chai": "^4.2.0",
    "ember-cli": "~3.8.1",
    "ember-cli-babel": "7.6.0",
    "ember-cli-dependency-checker": "^3.1.0",
    "eslint-plugin-node": "^7.0.1",
    "fixturify-project": "^1.8.0",
    "mocha": "^5.2.0",
    "strip-indent": "^2.0.0",
