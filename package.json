{
  "name": "ember-cli-addon-guard",
  "version": "0.0.1",
  "description": "Protect your app against common addon dependency problems.",
  "keywords": [
    "ember-addon"
  ],
  "repository": "dgeb/ember-cli-addon-guard",
  "license": "MIT",
  "authors": [
    "Dan Freeman",
    "Dan Gebhardt"
  ],
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "mocha tests-node/{unit,acceptance}/**/*-test.js",
    "test:all": "ember try:each"
  },
  "dependencies": {
    "archy": "^1.0.0",
    "broccoli-plugin": "^1.3.1",
    "chalk": "^2.4.2",
    "silent-error": "^1.1.1"
  },
  "devDependencies": {
    "@ember/optional-features": "^0.6.3",
    "broccoli-asset-rev": "^2.7.0",
    "chai": "^4.2.0",
    "ember-cli": "~3.8.1",
    "ember-cli-addon-tests": "^0.11.0",
    "ember-cli-babel": "7.6.0",
    "ember-cli-dependency-checker": "^3.1.0",
    "ember-cli-eslint": "^4.2.3",
    "ember-cli-inject-live-reload": "^1.8.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-template-lint": "^1.0.0-beta.1",
    "ember-cli-uglify": "^2.1.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^1.1.0",
    "ember-qunit": "^3.4.1",
    "ember-resolver": "^5.0.1",
    "ember-source": "~3.8.0",
    "ember-source-channel-url": "^1.1.0",
    "ember-try": "^1.0.0",
    "eslint-plugin-ember": "^5.2.0",
    "eslint-plugin-node": "^7.0.1",
    "fixturify-project": "^1.8.0",
    "loader.js": "^4.7.0",
    "mocha": "^5.2.0",
    "qunit-dom": "^0.8.0",
    "strip-indent": "^2.0.0"
  },
  "engines": {
    "node": "6.* || 8.* || >= 10.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
